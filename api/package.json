{
  "name": "double-take-api",
  "version": "1.13.1",
  "bin": "server.js",
  "description": "Unified UI and API for processing and training images for facial recognition",
  "scripts": {
    "start": "bun server.js",
    "debug": "nodemon --exec 'bun' server.js",
    "preinstall": "npm install --build-from-source better-sqlite3@'^8.2.0' --sqlite3=\"/src/sqlite\" && npm install --build-from-source canvas"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skrashevich/double-take.git"
  },
  "author": "David Jakowenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skrashevich/double-take/issues"
  },
  "homepage": "https://github.com/skrashevich/double-take#readme",
  "pkg": {
    "assets": [
      "views/**/*",
      "/usr/lib/x86_64-linux-gnu/libgif.so.7"
    ],
    "targets": [
      "node16-linux"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "@aws-sdk/client-rekognition": "^3.105.0",
    "@mhoc/axios-digest-auth": "^0.8.0",
    "archiver": "^5.3.1",
    "axios": "^0.27.2",
    "better-sqlite3": "^8.2.0",
    "canvas": "^2.11.0",
    "cors": "^2.8.5",
    "execution-time": "^1.4.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "form-data": "^4.0.0",
    "get-orientation": "^1.1.2",
    "joi": "^17.6.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^19.0.0",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "luxon": "^2.5.2",
    "mqtt": "^4.3.7",
    "multer": "^1.4.5-lts.1",
    "node-schedule": "^2.1.0",
    "probe-image-size": "^7.2.3",
    "read-last-lines": "^1.8.0",
    "romanization": "^0.0.1",
    "sharp": "^0.30.4",
    "socket.io": "^4.5.1",
    "traverse": "^0.6.6",
    "uuid": "^8.3.2",
    "winston": "^3.7.2"
  }
}
